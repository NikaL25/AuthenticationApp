@page
@model AuthenticationApp.Pages.UserModel
@{
}

<h2 class="text-center">Welcome to your account</h2>

<div>
    First Name: @Model.appUser?.FirstName
</div>
<div>
    Last Name: @Model.appUser?.LastName
</div>
<div>
    Email Name: @Model.appUser?.Email
</div>
<div>
    Phone Name: @Model.appUser?.PhoneNumber
</div>
<div>
    Address Name: @Model.appUser?.Address
</div>
<div>
    Created At Name: @Model.appUser?.CreatedAt.ToString("MM/dd/yyyy")
</div>
